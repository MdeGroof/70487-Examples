<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="ConService.CallService">
        <endpoint address="status"
                  binding="wsHttpBinding"
                  contract="ConService.IConService" />
        <endpoint address="mex"
                  binding="mexHttpBinding"
                  contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:11001/" />
          </baseAddresses>
        </host>
      </service>
      <service name="ConService.SessionService">
        <endpoint address="status"
                  binding="wsHttpBinding"
                  contract="ConService.IConService" />
        <endpoint address="mex"
                  binding="mexHttpBinding"
                  contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:11002/" />
          </baseAddresses>
        </host>
      </service>
      <service name="ConService.SingleService">
        <endpoint address="status"
                  binding="wsHttpBinding"
                  contract="ConService.IConService" />
        <endpoint address="mex"
                  binding="mexHttpBinding"
                  contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:11003/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>